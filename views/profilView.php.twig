<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>OOP MVC</title>
    <link rel="stylesheet" href="web/home/stylehome.css">
    <link rel="stylesheet" href="web/header/styleheader.css">
    <link rel="stylesheet" href="web/profilview/profilview.css">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <script src="web/articleView.js"></script>
    <script src="web/profilView.js"></script>
</head>
<body>

{% block content %}
    <div class="profilview-view">
        <div class="profil">
            <h1>Profil Utilisateur :</h1>
            {% for i in profils %}
                {% if not loop.first and not loop.last %}
                    <div> {{ i }}</div>
                {% endif %}
            {% endfor %}
        </div>
    </div>
    <div class="content"></div>

{% endblock %}

</body>
</html>